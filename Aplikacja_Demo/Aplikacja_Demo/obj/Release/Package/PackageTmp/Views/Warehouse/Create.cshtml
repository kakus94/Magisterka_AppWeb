@model Aplikacja_Demo.Models.Warehouse.Supplier

<div id="modal_testCreate" class="modal-content modal-dialog modal-dialog-centered" style="z-index: inherit;">
    <div class="modal-header">
        <button id="closeModalx" type="button" class="close" data-dismiss="modal" onclick="closeModel()">&times;</button>
        <h4 class="modal-title">Stwórz Nowe Zamówienie</h4>
    </div>
    <div class="modal-body">
        @using (Html.BeginForm())
        {
            <fieldset>
                <div class="container">
                    <div class="col-md-3">
                        <div class="c">
                            @Html.LabelFor(model => model.name)
                            @Html.TextBoxFor(model => model.name, new { @class = "form-control" })
                        </div>
                        <div class="editor-label">
                            @Html.LabelFor(model => model.quantity)
                            @Html.TextBoxFor(model => model.quantity, new { @class = "form-control" })
                        </div>
                        <div class="editor-label">
                            @Html.LabelFor(model => model.rfid)
                            @Html.TextBoxFor(model => model.rfid, new { @class = "form-control" })
                        </div>
                        <div class="editor-label">
                            @Html.LabelFor(model => model.who)
                            @Html.TextBoxFor(model => model.who, new { @class = "form-control" })
                        </div>
                    </div>
                    <div class="col-md-3">
                        @Html.LabelFor(model => model.disctription)
                        @Html.TextAreaFor(model => model.disctription, new { @class = "form-control", @style = "height: 225px;" })
                    </div>
                </div>
            </fieldset>
            <div class="modal-footer">
                <div class="col-md-1">
                    <input type="submit" class="btn btn-primary" value="Zapisz zmiany" />
                </div>

                <div class="col-md-11">
                    <button id="closeModalCreate" type="button" class="btn btn-default" data-dismiss="modal" onclick="closeModel()">Close</button>
                </div>
            </div>
        }
    </div>
</div>